{
    "datastoreName": "rtm_complex_store",
    "datastoreStorage": {
        "customerManagedS3": {
            "bucket": "ruiliang-smarthome-iot-analytics-datastore",
            "roleArn": "arn:aws:iam::account_id:role/service-role/iot-analytics-datastore-role"
        }
    },
    "fileFormatConfiguration": {
        "parquetConfiguration": {
            "schemaDefinition": {
                "columns": [
                    {
                        "name": "vin",
                        "type": "string"
                    },
                    {
                        "name": "event_time",
                        "type": "timestamp"
                    },
                    {
                        "name": "data",
                        "type": "ARRAY < STRUCT < spouse: STRING, signals: ARRAY <STRING> > >"
                    }
                ]
            }
        }
    }
}